# Database
DATABASE_PATH=./data/aico.db

# Server
PORT=3000
HOST=localhost

# Security
SESSION_SECRET=your-session-secret-here
API_KEY=your-api-key-here
JWT_SECRET=your-jwt-secret-here
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Media Upload
UPLOAD_DIR=./uploads
MAX_UPLOAD_SIZE=30000000 # 30MB in bytes

# Logging
LOG_LEVEL=info
LOG_DIR=./logs